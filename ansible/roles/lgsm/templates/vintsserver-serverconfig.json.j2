{
  "ServerName": "{{ game_config.serverName | default(server_name) }}",
  "ServerDescription": "Managed by Discord Server Manager",
  "WelcomeMessage": "Welcome to {{ game_config.serverName | default(server_name) }}!",
  "Port": {{ ports.game }},
  "MaxClients": {{ game_config.maxPlayers | default(16) }},
  "Password": "{{ game_config.password | default('') }}",
  "PublicVisible": {{ game_config.public | default(true) | ternary('true', 'false') }},
  "WorldName": "{{ game_config.worldName | default('default') }}",
{% if game_config.worldSeed is defined and game_config.worldSeed %}
  "WorldSeed": "{{ game_config.worldSeed }}",
{% endif %}
  "WorldPlayStyle": "surviveandbuild",
  "AllowPvP": false,
  "AllowFireSpread": true,
  "Language": "en",
  "ServerLanguage": "en",
  "MaxChunkRadius": 12,
  "TickTime": 33.333,
  "SpawnRadius": 5000,
  "BlockTickInterval": 300,
  "MapSizeY": 256,
  "PassTimeWhenEmpty": false,
  "Roles": [
    {"Code": "suvisitor", "Name": "Survival Visitor", "PrivilegeLevel": -1, "Privileges": ["chat"], "DefaultGameMode": 1},
    {"Code": "crvisitor", "Name": "Creative Visitor", "PrivilegeLevel": -1, "Privileges": ["chat"], "DefaultGameMode": 2},
    {"Code": "limitedsuplayer", "Name": "Limited Survival Player", "PrivilegeLevel": 0, "Privileges": ["build", "useblock", "pickup", "chat", "attackcreatures", "attackplayers"], "DefaultGameMode": 1},
    {"Code": "limitedcrplayer", "Name": "Limited Creative Player", "PrivilegeLevel": 0, "Privileges": ["build", "useblock", "pickup", "chat", "attackcreatures", "attackplayers", "flymode"], "DefaultGameMode": 2},
    {"Code": "suplayer", "Name": "Survival Player", "PrivilegeLevel": 0, "Privileges": ["build", "useblock", "pickup", "chat", "attackcreatures", "attackplayers"], "LandClaimAllowance": 4, "LandClaimMaxAreas": 3, "DefaultGameMode": 1},
    {"Code": "crplayer", "Name": "Creative Player", "PrivilegeLevel": 0, "Privileges": ["build", "useblock", "pickup", "chat", "attackcreatures", "attackplayers", "flymode"], "LandClaimAllowance": 40, "LandClaimMaxAreas": 60, "DefaultGameMode": 2},
    {"Code": "sumod", "Name": "Survival Moderator", "PrivilegeLevel": 200, "Privileges": ["build", "useblock", "pickup", "chat", "kick", "ban", "whitelist", "attackcreatures", "attackplayers", "freemove", "gamemode", "flymode"], "DefaultGameMode": 1},
    {"Code": "crmod", "Name": "Creative Moderator", "PrivilegeLevel": 200, "Privileges": ["build", "useblock", "pickup", "chat", "kick", "ban", "whitelist", "attackcreatures", "attackplayers", "freemove", "gamemode", "flymode"], "DefaultGameMode": 2},
    {"Code": "admin", "Name": "Admin", "PrivilegeLevel": 99999, "AutoGrant": true, "Privileges": ["*"], "DefaultGameMode": 1}
  ],
  "DefaultRoleCode": "suplayer",
  "AntiAbuse": 0,
  "OnlyWhitelisted": false,
  "WhitelistMode": 1,
  "DefaultGameMode": 1,
  "CorruptionProtection": true,
  "RepairMode": false,
  "SkipEveryChunkRow": 0,
  "SkipEveryChunkRowWidth": 0,
  "StartupCommands": "",
  "ModPaths": ["Mods"],
  "ModDbUrl": "",
  "HostedMode": true,
  "HostedModeAllowMods": true,
  "NormalDamageToPlayers": true,
  "ChatRateLimitMs": 1000,
  "DieBelowDiskSpaceMb": 400,
  "NextPlayerGroupUid": 10
}
